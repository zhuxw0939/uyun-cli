{
  "name": "school-app",
  "version": "0.2.0",
  "private": false,
  "scripts": {
    "start": "NODE_ENV=production DEBUG=mydebug:* pm2 start app.json",
    "start_dev": "NODE_ENV=development DEBUG=mydebug:* pm2 start app.json",
    "zip": "grunt myzip",
    "unzip": "grunt myunzip",
    "linux": "cross-env NODE_ENV=production DEBUG=mydebug:* node ./app.js",
    "linux_dev": "cross-env NODE_ENV=development DEBUG=mydebug:* node ./app.js",
    "windows": "set DEBUG=mydebug:* cross-env NODE_ENV=production & supervisor -p 2000 -i src,.logs,.idea,.git,node_modules ./app.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --inline --hot",
    "build": "NODE_ENV=production webpack --progress --hide-modules",
    "build_windows": "cross-env NODE_ENV=production webpack --progress --hide-modules"
  },
